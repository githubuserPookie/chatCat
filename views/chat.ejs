<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="../public/stylesheets/chat.css">
</head>
<body>
    <h1 id="yourFriend">
    <a href="/"><img class="logo" src="../public/images/catPawImg.png" alt=""></a>Public Chat</h1>
    <div id="containFormMessageLog">
      <div id="messageLog">
      </div>
      <form action="" id="form" autocomplete="off">
          <input type="search" name="" id="message" placeholder="Enter your message">
      </form>
    </div>
    <div id="containerForImages">
      <div id="errorImg">
      <img
        src="../public/images/RedError.png"

        alt="Invalid message" />
      </div>
      <div id="warningImg">
      <img
        src="../public/images/OrangeCaution.png"

        alt="Possible invalid message" />
      </div>
      <div id="cursingImg">
      <img
        src="../public/images/Cursing.png"
        id="cursingImg"
        alt="Don't curse" />
      </div>
    </div>
    <script src="socket.io/socket.io.js"></script>
    <script type="module" src="../public/scripts/chat.js"></script>
    <script type="module" src="../public/scripts/functions/messageRelated.js"></script>
</body>
</html>